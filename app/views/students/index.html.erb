<h1> All Students: </h1>
<% @students.each do |student| %>
    <h3> <%=link_to "#{student.name}", student_path(student) %> </h3>
<% end %>

<h1> Students with Courses: </h1>
<% @enrolled_students.each do |student| %>
    <h3> <%=link_to "#{student.name}", student_path(student) %> </h3>
<% end %>

<h1> <%= link_to "Add a student to the school", new_student_path %> </h1>
